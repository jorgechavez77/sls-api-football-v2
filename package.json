{
  "name": "sls-api-football-v2",
  "description": "Serverless API for Football v2",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "joi": "^18.0.1",
    "mongodb": "^6.19.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@types/aws-lambda": "^8.10.150",
    "@types/node": "^22",
    "babel-jest": "^30.0.5",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "scripts": {
    "test:create": "sam local invoke CreateTeamFunction -e events/create-team.json",
    "test:get": "sam local invoke GetTournamentFunction -e events/get-team.json",
